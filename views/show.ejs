<% include partials/header.ejs%>

<h1>Kill List:</h1>


<div id="accordion">
<% posts.forEach(function (post) { %>
  <h4><%= post.name %></h4>
  <div>
    <p>
      <img id="pic" src= <%= post.photo%> />
      <p>
        age: <%= post.age %>
      </p>
      <% if (post.Assassin){ %>
      <p> 
        assassin: <%= post.Assassin.dataValues.title%>
      </p>
      <%}%>

      <h6><%= post.location %></h6>
    </p>
  </div>
<% }) %> 
</div>


<script>
  $( window ).load( function() {
    $( "#accordion" ).accordion();
  });
</script>


<% include partials/footer.ejs%>
