<!doctype html>
<html>
  <head>
    <title>Blog</title>
  </head>
  <body>
    <h1>Blog</h1>
      <% posts.forEach(function (post) { %>
      <%= console.log(posts) %>
      <div>
        <h2><%= post.title %></h2>
        <% if (user.username) { %>
        <p>by: <a href=""><%=user.username%></a></p>
        <% } else { %>
        <p>by: Anonymous</p>
        <% } %>
        <p><%= post.body %></p>
      </div>
      <% }) %>
  </body>
</html>